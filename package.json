    "dotenv": "^16.4.5",
    "env": "^0.0.2",
    "express": "^4.17.1",
    "jsonwebtoken": "^9.0.2",
    "node-fetch": "^2.7.0",
    "node-localstorage": "^3.0.5",
    "nodemon": "^3.1.4",
