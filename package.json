  "description": "",
  "main": "server.ts",
  "scripts": {
    "start": "tsx src/server.ts",
    "server": "nodemon src/server.ts",
    "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess \"node ./dist/server.js\"",
    "create-test-db": "ts-node src/models/tests/createTestDatabase.ts",
