{
  "spec_dir": "src",
  "spec_files": ["../**/tests/*[s]pec.ts", "**/*[tT]ests.js", "**/*[sS]rc.js"],
  "helpers": ["helpers/**/*.js"],
  "reporters": [
    {
      "name": "jasmine-spec-reporter#SpecReporter",
      "options": {
        "displayStacktrace": "all"
      }
    }
  ],
  "stopSpecOnExpectationFailure": false,
  "random": true
}
